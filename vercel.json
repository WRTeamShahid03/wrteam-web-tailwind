{
  "rewrites": [
    {
      "source": "/",
      "destination": "/landing-pages/index"
    }
  ],
  "headers": [
    {
      "source": "/landing-pages/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ]
}